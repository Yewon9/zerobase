      }
    },
    {
      "cell_type": "code",
      "source": [
        "nums = []\n",
        "\n",
        "n = 1\n",
        "while n < 6:\n",
        "  try :\n",
        "    num = int(input('input number : '))\n",
        "  except:\n",
        "    print('예외 발생!')\n",
        "    continue\n",
        "\n",
        "  nums.append(num)\n",
        "  n += 1\n",
        "\n",
        "print(f'nums : {nums}')"
      ],
